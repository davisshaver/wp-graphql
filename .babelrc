{
  "presets": [
    "es2015",
    "stage-0"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "type": "./schema/type",
        "enum": "./schema/enum",
        "interface": "./schema/interface",
        "query": "./schema/query",
        "field": "./schema/field",
        "data": "./schema/data",
        "utils": "./schema/utils"
      }
    }]
  ]
}
